{
  "name": "tabemano-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nest start --watch",
    "test": "jest",
    "start:dev-old": "nodemon",
    "db:seed": "ts-node ./prisma/seed.ts",
    "db:populate": "ts-node ./prisma/fake-data-seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:schema": "prisma generate",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:create": "cd typeorm/migration && typeorm migration:create",
    "typeorm:seed": "ts-node -r tsconfig-paths/register ./typeorm/script/seed.ts",
    "typeorm:dev-seed": "ts-node -r tsconfig-paths/register ./typeorm/script/dev-seed.ts",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -d ./typeorm/config.ts",
    "typeorm:migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:revert -d ./typeorm/config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@badrap/result": "^0.2.12",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/cqrs": "^8.0.4",
    "@nestjs/platform-express": "^8.4.5",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/jest": "^29.0.0",
    "axios": "^0.26.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "cookie-session": "^2.0.0",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "merge-anything": "^5.0.2",
    "pg": "^8.7.3",
    "prisma": "^3.12.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typeorm": "^0.3.6",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.5",
    "@prisma/client": "^3.12.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.24",
    "@types/uuid": "^8.3.4",
    "jest": "^29.0.2",
    "nodemon": "^2.0.15",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
